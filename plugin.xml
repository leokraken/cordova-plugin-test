<?xml version="1.0" encoding="utf-8"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
        id="leo.example.menu"
        version="0.0.1">

  <name>Example</name>

  <engines>
    <engine name="cordova" version=">=3.4.0"/>
  </engines>

  <asset src="www/hello.js" target="js/hello.js"/>

  <js-module src="www/hello.js" name="hello">
    <clobbers target="hello" />
  </js-module>

  <platform name="android">

    <config-file target="res/xml/config.xml" parent="/*">
      <feature name="Hello">
        <param name="android-package" value="leo.example.menu.Wrapper"/>
      </feature>
    </config-file>

    <source-file src="app/src/main/java/leo/example/menu/Wrapper.java" target-dir="src/com/example/plugin/"/>
    <source-file src="app/src/main/java/leo/example/menu/MainActivity.java" target-dir="src/com/example/plugin/"/>
    <source-file src="app/src/main/res/layout/activity_main.xml" target-dir="res/layout"/>

  </platform>


</plugin>

